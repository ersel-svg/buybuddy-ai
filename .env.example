# ===========================================
# Buybuddy AI Platform - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Runpod
RUNPOD_API_KEY=your-runpod-api-key
RUNPOD_ENDPOINT_VIDEO=your-video-endpoint-id
RUNPOD_ENDPOINT_AUGMENTATION=your-augmentation-endpoint-id
RUNPOD_ENDPOINT_TRAINING=your-training-endpoint-id
RUNPOD_ENDPOINT_EMBEDDING=your-embedding-endpoint-id
RUNPOD_ENDPOINT_PREVIEW=your-preview-endpoint-id

# Object Detection Endpoints
RUNPOD_ENDPOINT_OD_ANNOTATION=your-od-annotation-endpoint-id
RUNPOD_ENDPOINT_OD_TRAINING=your-od-training-endpoint-id

# Classification Endpoints
RUNPOD_ENDPOINT_CLS_ANNOTATION=your-cls-annotation-endpoint-id
RUNPOD_ENDPOINT_CLS_TRAINING=your-cls-training-endpoint-id

# External APIs
GEMINI_API_KEY=your-gemini-api-key
HF_TOKEN=your-huggingface-token

# Buybuddy Legacy API
BUYBUDDY_API_URL=https://api-legacy.buybuddy.co/api/v1
BUYBUDDY_USERNAME=your-buybuddy-username
BUYBUDDY_PASSWORD=your-buybuddy-password

# Qdrant Vector Database
QDRANT_URL=https://your-cluster.qdrant.io
QDRANT_API_KEY=your-qdrant-api-key

# Application
NEXT_PUBLIC_API_URL=http://localhost:8000
NODE_ENV=development
DEBUG=false

# CORS (comma-separated list of allowed origins)
# Example: CORS_ORIGINS=["https://your-app.vercel.app","https://custom-domain.com"]

# Processing defaults
TARGET_RESOLUTION=518
